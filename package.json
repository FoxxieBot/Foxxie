{
  "name": "@foxxie/foxxie",
  "version": "0.0.0",
  "description": "Monorepository containing all the source code for the Foxxie Project.",
  "private": true,
  "scripts": {
    "build": "yarn nx affected:build",
    "test": "yarn nx affected:test",
    "lint": "yarn nx affected:lint",
    "upin": "yarn upgrade-interactive --latest",
    "graph": "yarn nx dep-graph",
    "affected": "yarn nx graph",
    "full:test": "jest --pass-with-no-tests --collect-coverage --verbose",
    "nx:b": "yarn nx build",
    "clean": "node scripts/clean.mjs",
    "delete-branches": "git branch  | egrep -v \"(^\\*|master|main)\" | xargs git branch -D"
  },
  "contributors": [
    "Ruffpuff1 <contact@ruffpuff.dev>",
    "Raindrop-Droptop"
  ],
  "keywords": [
    "bot",
    "npm",
    "nx",
    "packages",
    "monorepo",
    "yarn",
    "discord",
    "foxxie"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruffpuff1/foxxie.git"
  },
  "bugs": {
    "url": "https://github.com/Ruffpuff1/foxxie/issues"
  },
  "homepage": "https://github.com/Ruffpuff1/foxxie",
  "devDependencies": {
    "@nrwl/cli": "latest",
    "@nrwl/nx-cloud": "13.2.2",
    "@nrwl/workspace": "^13.10.0",
    "@octokit/action": "^3.18.0",
    "@types/eslint": "^8",
    "@types/skia-canvas": "^0.9.28",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "colorette": "^2.0.16",
    "conventional-changelog-angular": "^5.0.13",
    "conventional-recommended-bump": "^6.1.0",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "nx": "^13.10.1",
    "prettier": "^2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "tsup": "^5.12.3",
    "typescript": "^4.6.3"
  },
  "workspaces": [
    "apps/*",
    "graphql/*",
    "packages/*",
    "website/*"
  ],
  "packageManager": "yarn@1.22.18",
  "dependencies": {
    "@foxxie/ts-config": "^0.0.0"
  }
}
