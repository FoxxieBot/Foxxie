{
  "name": "@foxxie/commands",
  "version": "0.0.2",
  "description": "Interaction command registration for Foxxie, used exclusively with the Sapphire framework.",
  "scripts": {
    "build": "rm -r -f dist && tsup",
    "lint": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "test": "jest --pass-with-no-tests --collect-coverage --verbose"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "directories": {
    "lib": "src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruffpuff1/foxxie.git",
    "directory": "packages/commands"
  },
  "bugs": {
    "url": "https://github.com/Ruffpuff1/foxxie/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "foxxie",
    "commands",
    "discord",
    "decorators"
  ],
  "devDependencies": {
    "@foxxie/eslint-config": "^0.0.2",
    "@types/jest": "^27.4.1",
    "@types/latinize": "^0.2.15",
    "@types/node": "17.0.23",
    "@types/unidecode": "^0.1.1",
    "@types/unorm": "^1.3.28",
    "discord-api-types": "^0.31.0",
    "jest": "^27.5.1",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@foxxie/builders": "^0.0.0",
    "@foxxie/i18n": "^0.0.2",
    "@sapphire/decorators": "^4.3.4",
    "@sapphire/framework": "^3.0.0-next.379f5db.0",
    "discord.js": "^13.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
