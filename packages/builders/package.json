    "foxxie",
    "slashcommands",
    "builders"
  ],
  "devDependencies": {
    "@foxxie/eslint-config": "^0.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.23",
    "jest": "^27.5.1",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@foxxie/types": "^0.0.6",
    "i18next": "^21.6.16"
