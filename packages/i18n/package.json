{
  "name": "@foxxie/i18n",
  "version": "0.0.2",
  "description": "I18n management utility for Foxxie projects.",
  "scripts": {
    "build": "rm -r -f dist && tsup",
    "lint": "prettier --write . && eslint src --ext mjs,js,ts --fix"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
