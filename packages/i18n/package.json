    "require": "./dist/index.js"
  },
  "directories": {
    "lib": "src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruffpuff1/foxxie.git",
    "directory": "packages/i18n"
  },
  "bugs": {
    "url": "https://github.com/Ruffpuff1/foxxie/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "foxxie",
    "i18n",
    "translations",
    "internalization",
    "crowdin",
    "i10n"
  ],
  "devDependencies": {
    "@foxxie/eslint-config": "^0.0.2",
    "@ruffpuff/ts": "^1.0.4",
    "@types/i18next-fs-backend": "^1.1.2",
    "discord-api-types": "^0.31.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@discordjs/collection": "^0.5.0",
    "i18next": "^21.6.14",
    "i18next-fs-backend": "^1.1.4"
  },
  "publishConfig": {
    "access": "public"
  }
