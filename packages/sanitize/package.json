{
  "name": "@foxxiebot/sanitize",
  "version": "0.0.0",
  "description": "Sanitize and clean character strings.",
  "author": "Ruffpuff1",
  "scripts": {
    "build": "yarn gen-index && tsup && yarn build:rename-cjs-index",
    "build:rename-cjs-index": "tsx ../../scripts/rename-cjs-index.cts",
    "prepack": "yarn build",
    "gen-index": "tsx ../../scripts/gen-index.cts sanitize --write"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.cts",
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.mts",
      "default": "./dist/esm/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.cts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "license": "MIT",
  "keywords": [
    "foxxie",
    "sanitizer"
  ],
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=v16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@ruffpuff/eslint-config": "1.1.0",
    "@types/latinize": "^0.2.15",
    "@types/unidecode": "^0.1.1",
    "@types/unorm": "^1.3.28",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "confusables": "^1.1.1",
    "latinize": "^2.0.0",
    "remove-accents": "^0.5.0",
    "unidecode": "^1.1.0"
  }
}
