{
  "name": "@foxxie/fetch",
  "version": "0.0.4",
  "description": "A centra inspired http client for Foxxie bots",
  "scripts": {
    "build": "tsup",
    "lint": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "test": "jest --pass-with-no-tests --collect-coverage --verbose"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "directories": {
    "lib": "src",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruffpuff1/foxxie.git",
    "directory": "packages/fetch"
  },
  "bugs": {
    "url": "https://github.com/Ruffpuff1/foxxie/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "foxxie",
    "http",
    "fetch",
    "yarn"
  ],
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.25",
    "jest": "27.5.1",
    "nock": "13.2.7",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@foxxie/eslint-config": "^0.0.2",
    "@ruffpuff/utilities": "^4.0.0",
    "undici": "^5.0.0"
  }
}
