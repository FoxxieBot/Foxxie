{
  "name": "@foxxie/statuspage",
  "version": "0.0.0",
  "description": "A mini client wrapper for statuspage.io.",
  "scripts": {
    "build": "rm -r -f dist && tsup",
    "lint": "prettier --check . && eslint src --ext mjs,js,ts",
    "test": "jest --pass-with-no-tests --collect-coverage --verbose",
    "format": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "changelog": "rm -f CHANGELOG.md && touch CHANGELOG.md && git cliff --prepend ./CHANGELOG.md -l -c ./cliff.toml -r ../../ --include-path 'packages/statuspage/*'"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "directories": {
    "lib": "src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruffpuff1/foxxie.git",
    "directory": "packages/statuspage"
  },
  "bugs": {
    "url": "https://github.com/Ruffpuff1/foxxie/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "foxxie",
    "discord",
    "statuspage"
  ],
  "devDependencies": {
    "@foxxie/eslint-config": "^0.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "jest": "^27.5.1",
    "prettier": "2.6.1",
    "tsup": "5.12.1",
    "typescript": "4.6.3"
  },
  "private": false,
  "dependencies": {
    "@foxxie/fetch": "^0.0.4",
    "@ruffpuff/utilities": "^4.0.0"
  }
}
