{
  "additionalBranchPrefix": "{{parentDir}} -",
  "assigneesFromCodeOwners": true,
  "extends": ["github>sapphiredev/readme:sapphire-renovate"],
  "schedule": ["at any time"],
  "labels": ["Meta: Dependencies"],
  "npm": {
		"packageRules": [
			{
				"matchPackagePatterns": ["@influxdata"],
				"groupName": "InfluxDB Dependencies"
			},
      {
				"matchPackagePatterns": ["@ruffpuff"],
				"groupName": "Ruffpuff Dependencies"
			},
      {
				"matchPackagePatterns": ["@foxxie"],
				"groupName": "Foxxie Dependencies"
			}
		]
	}
}
