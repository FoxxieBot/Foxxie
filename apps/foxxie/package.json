{
    "name": "foxxie",
    "version": "5.0.0",
    "description": "A utility and moderation centered discord bot, to provide orginazation to your server.",
    "main": "./dist/Foxxie.js",
    "homepage": "https://github.com/Ruffpuff1/foxxie",
    "repository": {
        "type": "git",
        "url": "https://github.com/Ruffpuff1/foxxie",
        "directory": "apps/foxxie"
    },
    "keywords": [
        "foxxie",
        "discord",
        "bot",
        "moderation",
        "security"
    ],
    "scripts": {
        "test": "echo Testing",
        "start": "node .",
        "deps": "rm -f yarn.lock && rm -r -f node_modules && yarn",
        "lint": "prettier --write . && eslint src --ext mjs,js,ts --fix",
        "build": "yarn tsc -b",
        "clean": "rm -r node_modules && yarn",
        "watch": "tsc -b -w",
        "dev": "dotenv -e .env.development -- yarn build && yarn start",
        "watch:start": "tsc-watch -b --onSuccess \"yarn start\"",
        "clean:dist": "rm -r dist",
        "production": "NODE_ENV=production node .",
        "prisma:gen": "npx prisma generate",
        "prisma:pull": "dotenv -e .env.development -- npx prisma db pull"
    },
    "imports": {
        "#database/*": "./dist/lib/database/*.js",
        "#database/Keys": "./dist/lib/database/Keys/index.js",
        "#external/Api": "./dist/lib/util/External/Api.js",
        "#external/GuildMemberFetchQueue": "./dist/lib/util/External/GuildMemberFetchQueue.js",
        "#external/LongLivingReactionCollector": "./dist/lib/util/External/LongLivingReactionCollector.js",
        "#external/PaginatedMessage": "./dist/lib/util/External/PaginatedMessage.js",
        "#lib/*": "./dist/lib/*.js",
        "#lib/database": "./dist/lib/database/index.js",
        "#lib/prisma": "./dist/lib/prisma/index.js",
        "#lib/discord": "./dist/lib/discord/index.js",
        "#lib/FoxxieClient": "./dist/lib/FoxxieClient.js",
        "#lib/game": "./dist/lib/game/index.js",
        "#lib/i18n": "./dist/lib/i18n/index.js",
        "#lib/setup": "./dist/lib/setup/index.js",
        "#lib/structures": "./dist/lib/structures/index.js",
        "#lib/types": "./dist/lib/types/index.js",
        "#languages": "./dist/languages/index.js",
        "#root/*": "./dist/*.js",
        "#root/config": "./dist/config.js",
        "#utils/*": "./dist/lib/util/*.js",
        "#utils/API": "./dist/lib/util/API/index.js",
        "#utils/birthday": "./dist/lib/util/birthday.js",
        "#utils/constants": "./dist/lib/util/constants.js",
        "#utils/Discord": "./dist/lib/util/Discord/index.js",
        "#utils/moderation": "./dist/lib/util/moderation.js",
        "#utils/regexCreator": "./dist/lib/util/regexCreator.js",
        "#utils/transformers": "./dist/lib/util/transformers.js",
        "#utils/util": "./dist/lib/util/util.js",
        "#utils/decorators": "./dist/lib/util/decorators.js",
        "#external/*": "./dist/lib/util/External/*.js"
    },
    "funding": {
        "type": "individual",
        "url": "https://ko-fi.com/ruffpuff"
    },
    "author": "ruffpuff",
    "license": "MIT",
    "dependencies": {
        "@discordjs/builders": "^0.13.0",
        "@favware/graphql-pokemon": "6.5.0",
        "@foxxie/api": "0.0.3",
        "@foxxie/commands": "^0.0.3",
        "@foxxie/env": "^0.0.2",
        "@foxxie/fetch": "0.0.4",
        "@foxxie/i18n": "^0.0.4",
        "@foxxie/lock-queue": "0.1.2",
        "@foxxie/sanitize": "^0.0.0",
        "@influxdata/influxdb-client": "^1.24.0",
        "@prisma/client": "^3.12.0",
        "@ruffpuff/api": "^0.0.2-next.ce2ba26.1647722130",
        "@ruffpuff/ts": "^1.0.4",
        "@ruffpuff/utilities": "^4.0.0",
        "@sapphire/decorators": "^4.3.4",
        "@sapphire/discord.js-utilities": "^4.9.4",
        "@sapphire/framework": "^3.0.0-next.379f5db.0",
        "@sapphire/plugin-editable-commands": "^1.1.3-next.db15252.0",
        "@sapphire/plugin-logger": "^2.2.2-next.02c4555.0",
        "@sapphire/plugin-scheduled-tasks": "^2.3.2",
        "@sapphire/plugin-subcommands": "2.2.0",
        "@sapphire/ratelimits": "^2.4.4",
        "@sapphire/stopwatch": "^1.4.1",
        "@sapphire/type": "^2.2.0",
        "@sapphire/utilities": "^3.6.2",
        "@sentry/node": "^6.19.6",
        "@skyra/char": "^1.0.2",
        "@skyra/jaro-winkler": "^1.1.0",
        "bull": "^4.8.1",
        "canvas-constructor": "^5.0.2",
        "colorette": "^2.0.16",
        "discord.js": "^13.6.0",
        "dotenv-cra": "^3.0.2",
        "i18next": "^21.6.16",
        "ioredis": "^5.0.3",
        "lexure": "^0.17.0",
        "moment-timezone": "^0.5.34",
        "mongodb": "^3.7.3",
        "skia-canvas": "^0.9.29",
        "typeorm": "^0.2.45"
    },
    "devDependencies": {
        "@foxxie/eslint-config": "^0.0.2",
        "@foxxie/types": "^0.0.6",
        "@types/bull": "3.15.8",
        "@types/ioredis": "4.28.10",
        "@types/node": "17.0.24",
        "@types/spotify-api": "0.0.14",
        "@types/tinycolor2": "1.4.3",
        "@typescript-eslint/eslint-plugin": "5.20.0",
        "@typescript-eslint/parser": "5.19.0",
        "discord-api-types": "0.31.2",
        "dotenv-cli": "^5.1.0",
        "eslint": "8.13.0",
        "prisma": "^3.12.0",
        "ruffpuff-api-types": "^0.0.5-next.11a326b.1647721912",
        "tsc-watch": "5.0.3",
        "typescript": "^4.6.3"
    }
}
